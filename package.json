{
    "name": "@fangzhongya/vue-components",
    "private": false,
    "type": "module",
    "version": "0.1.2-7",
    "description ": "unplugin-vue-components插件",
    "author": "fangzhongya ",
    "license": "MIT",
    "keywords": [
        "unplugin-vue-components插件"
    ],
    "repository": {
        "type": "git",
        "url": " https://github.com/fangzhongya/vue-components.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "dev": "ts-node node.index.ts",
        "dev:tests": "ts-node node.tests.ts",
        "build": "tsup",
        "test": "vitest test"
    },
    "devDependencies": {
        "@fangzhongya/create": "0.2.0-6",
        "@fangzhongya/utils": "0.0.7-2",
        "@types/node": "^18.11.19",
        "tsup": "^6.5.0",
        "typescript": "^4.9.5",
        "unplugin-vue-components": "^0.23.0",
        "vitest": "^0.28.4"
    },
    "dependencies": {
        "fast-glob": "^3.2.12"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "exports": {
        "./archive": {
            "require": "./dist/archive.cjs",
            "import": "./dist/archive.js",
            "types": "./dist/archive.d.ts"
        },
        "./resolver": {
            "require": "./dist/resolver.cjs",
            "import": "./dist/resolver.js",
            "types": "./dist/resolver.d.ts"
        },
        "./component": {
            "require": "./dist/component.cjs",
            "import": "./dist/component.js",
            "types": "./dist/component.d.ts"
        },
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./index": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    }
}
